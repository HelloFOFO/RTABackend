<%include header%>
<!--！！！！！！！！！！！！！！这个头都是一样的，可以全部进行替换！！！！！！！！！！！！！！-->
<style>
    #createModal .modal-dialog{
        width:800px;
    }
</style>
<div class="container-fluid">
    <div class="row">

        <%include modules%>

        <div class="col-md-10 col-md-offset-2 main">
            <div class="row">
                <!--这一部分为查询栏-->
                <div class="col-md-12">
                    <form class="form-horizontal" role="form" method="GET" id="queryForm">
                        <div class="form-group">
                            <label class="control-label col-md-1" for="searchProvider">企业：</label>
                            <div class="col-md-2">
                                <select id="searchProvider" name="searchProvider" class="form-control">
                                    <option value="">__请选择企业__</option>
                                    <%providerInfo.forEach(function(obj){%>
                                    <option value="<%=obj._id%>"><%=obj.name%></option>
                                    <%});%>
                                </select>
                            </div>

                        </div>
                        <div class="form-group">
                            <label class="control-label col-md-1" for="searchMember">账号：</label>
                            <div class="col-md-2">
                                <select id="searchMember" name="searchMember" class="form-control">

                                </select>
                            </div>
                            <label class="control-label col-md-1" for="searchModule">模块：</label>
                            <div class="col-md-2">
                                <select id="searchModule" name="searchModule" class="form-control">
                                    <%moduleInfo.forEach(function(obj){%>
                                    <option value="<%=obj._id%>"><%=obj.cat%>-><%=obj.name%></option>
                                    <%});%>
                                </select>
                            </div>
                        </div>
                        <div class="form-group">
                            <div class="col-md-1 col-md-offset-1">
                                <button id="query" class="btn btn-danger form-control" type="submit">配置</button>
                            </div>
                        </div>
                    </form>
                </div>
            </div>



            <div class="row">
                <!--这一部分是按钮栏-->
                <div class="col-md-12">

                </div>
            </div>
            <div class="row" id="queryResult">
                <!--这一部分是分页栏 以及 表格栏-->

            </div>
        </div>
    </div>
</div>

</body>

<%include footer%>

<script src="/js/common/rightsManagement.js"></script>



<script>
    $(document).ready(function() {
    } );
</script>

</html>